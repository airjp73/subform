{
  "name": "subform",
  "version": "0.0.4",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./zod": {
      "types": {
        "import": "./dist/zod/index.d.mts",
        "require": "./dist/zod/index.d.ts"
      },
      "import": "./dist/zod/index.mjs",
      "default": "./dist/zod/index.js"
    },
    "./react": {
      "types": {
        "import": "./dist/react/index.d.mts",
        "require": "./dist/react/index.d.ts"
      },
      "import": "./dist/react/index.mjs",
      "default": "./dist/react/index.js"
    }
  },
  "files": [
    "**"
  ],
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc",
    "test": "vitest",
    "build": "tsup",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@testing-library/user-event": "^14.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.38.0",
    "jsdom": "^22.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.1.6",
    "zustand": "^4.4.6",
    "vitest": "^0.34.6",
    "remeda": "^1.29.0",
    "tiny-invariant": "^1.3.1"
  },
  "peerDependencies": {
    "zod": ">=3.0.0 <4.0.0",
    "@types/react": ">=18.0.0 <19.0.0",
    "react": ">=18.0.0 <19.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
